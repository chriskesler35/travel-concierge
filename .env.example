# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Legacy API Configuration (if needed for external services)
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_TIMEOUT=30000

# Security
VITE_ENABLE_HTTPS=true

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_PREMIUM_FEATURES=true

# External Services (replace with your own)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Email Service
VITE_EMAIL_SERVICE_URL=http://localhost:3001/email
VITE_EMAIL_FROM_ADDRESS=noreply@travelconcierge.com

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,application/pdf